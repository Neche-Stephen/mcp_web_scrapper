  "package_url": "https://github.com/Arenukvern/mcp_flutter",
  "source_url": "",
  "language": "JavaScript",
  "server_configuration": [
    {
      "name": "LOG_LEVEL",
      "required": "No",
      "description": "Logging level (debug, info, notice, warning, error, critical, alert, emergency)",
      "default": "critical"
    },
    {
      "name": "DART_VM_HOST",
      "required": "No",
      "description": "Dart VM server observablity host",
      "default": "localhost"
    },
    {
      "name": "DART_VM_PORT",
      "required": "No",
      "description": "Server port",
      "default": "8181"
    },
    {
      "name": "MCP_SERVER_HOST",
      "required": "No",
      "description": "This server host",
      "default": "localhost"
    },
    {
      "name": "MCP_SERVER_PORT",
      "required": "No",
      "description": "This server port",
      "default": "3535"
    },
    {
      "name": "FORWARDING_SERVER_HOST",
      "required": "No",
      "description": "Forwarding server host to connect Flutter Devtools Extension with MCP Server",
      "default": "localhost"
    },
    {
      "name": "FORWARDING_SERVER_PORT",
      "required": "No",
      "description": "Forwarding server port to connect Flutter Devtools Extension with MCP Server",
      "default": "8143"
    }
  ],
  "github_url": "https://github.com/Arenukvern",
  "scrape_source": "https://glama.ai/mcp/servers/@Arenukvern/mcp_flutter"
}