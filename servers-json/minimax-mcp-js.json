  "server_configuration": [
    {
      "name": "MINIMAX_API_KEY",
      "required": "Yes",
      "description": "MiniMax API Key (required)",
      "default": ""
    },
    {
      "name": "MINIMAX_API_HOST",
      "required": "No",
      "description": "MiniMax API Host",
      "default": "https://api.minimaxi.chat"
    },
    {
      "name": "MINIMAX_MCP_BASE_PATH",
      "required": "No",
      "description": "Base path for output files",
      "default": "User's desktop"
    },
    {
      "name": "MINIMAX_RESOURCE_MODE",
      "required": "No",
      "description": "Resource handling mode, 'url' or 'local'",
      "default": "url"
    }
  ],
  "github_url": "https://github.com/MiniMax-AI",
